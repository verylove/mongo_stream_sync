{
    "mongo": {
      "options": {
        "poolSize": 100,
        "useNewUrlParser": true
      },
      "url": "localhost:27011",
      "database": "refresh_tmp_db",
      "user": "",
      "collections": ["Product"]
    },
    "elasticsearch": {
      "host": "localhost:9200",
      "apiVersion": "2.4"
    },
    "bulkSize": 1000,
    "adminPort": 3000,
    "ignoreResumeTokensOnStart": true,
    "dumpOnStart": false,
    "resumeTokenInterval": 60000,
    "resumeTokenCollection": "resumeToken",
    "dumpProgressCollection": "dumpProgress",
    "mappings": {
      "Product": {
        "index": "product-index",
        "type": "product",
        "versionType": "external",
        "versionField": "updatedAt"
      }
    },
    "logLevel": "info"
  }